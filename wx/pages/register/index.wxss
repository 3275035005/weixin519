/* --- logo --- */
.register-logo {text-align: center; background: #5460FE; height: 120rpx; position: relative; z-index: 1;}
.register-logo .box{width: 200rpx; height: 200rpx; border-radius: 100rpx; background: #fff; margin: 0 auto;}
.register-logo .box image{width: 262rpx; height: 176rpx; margin-left: -30rpx;}



/* --- column --- */
.register-column{text-align: center; padding: 20rpx 0;}
.register-column .title{font-size: 64rpx; font-weight: bold;}
.register-column .dep{color: #979797; opacity: .4; font-family: 'DIN-Bold';}



/* --- box --- */
.register-box{background: #fff; border-radius: 32rpx 32rpx 0 0; margin-top: -20rpx; position: relative; z-index:0;}

