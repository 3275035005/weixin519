/**app.wxss**/

/* --- all --- */
.padding{padding: 32rpx;}
.flex{display: flex; align-items: center; flex-wrap: wrap;}
.center{justify-content: center;}
.shadow{box-shadow: 0 1px 2px rgba(209,218,235,0.5);}
.placeholder{color: #ccc;}



/* --- column --- */
.page_column{justify-content: space-between;}
.page_column__title{font-size: 32rpx; position: relative; z-index: 99;}
.page_column__title::after{content: ""; left: 0; width: 100%; background: #467EF5; height: 8rpx; position: absolute; bottom: 0; background-color:rgba(70,126,245,0.5); }
.page_column__more{font-size: 24rpx; opacity: .6;}



/* --- button --- */
.page_button{background: linear-gradient(to bottom, #4564FC , #5F7AFB); color: #fff; border-radius: 20rpx; height: 112rpx; line-height: 112rpx; padding: 0; font-weight: 400; font-size: 28rpx;}
.page_button__icon{width: 40rpx; height: 40rpx; margin-right: 16rpx;}

button[size=mini]{padding: 0 0;}



/* --- 公司产品 --- */
.page_product{margin: 30rpx 32rpx;}
.page_product__list{margin-top: 32rpx; justify-content: space-between;}
.page_product__item{width: 328rpx; margin-bottom: 32rpx;}
.page_product__image{width: 328rpx; height: 203.36rpx; margin-bottom: 12rpx;}
.page_product__title{font-size: 28rpx; margin-bottom: 8rpx; overflow:hidden;  white-space: nowrap;  text-overflow: ellipsis;}
.page_product__other{font-size: 24rpx;}
.page_product__price{margin-right: 16rpx; color: #FF4C4C;}
.page_product__old{text-decoration: line-through; color: #999;}




/* --- tab --- */
.page_tab{background: #fff; height: 96rpx; border-bottom: 1rpx solid #efefef;}
.page_tab__item{font-size: 28rpx; border-right: 1rpx solid #efefef; box-sizing: border-box; position: relative; height: 100%; display: flex; align-items: center; justify-content: center;}
.page_tab__item:last-child{border-right: none;}
.page_tab__item::before{content: ""; left: 40%; width: 0%; bottom: 0; background: #FF4C4C; height: 4rpx; position: absolute; bottom: 0;}

/* active */
.page_tab__item.active{color: #FF4C4C;}
.page_tab__item.active::before{width: 20%;}

/* three */
.page_tab.three .page_tab__item{width: 33.3333%; text-align: center;}

/* two */
.page_tab.two .page_tab__item{width: 50%; text-align: center;}

/* fore */
.page_tab.fore .page_tab__item{width: 25%; text-align: center;}





/* --- nothing --- */
.page_nothing{height: calc(100vh - 88rpx); flex-flow: column; align-items: center;}
.page_nothing__icon{width: 320rpx; height: 320rpx; margin-top: 40%;}
.page_nothing__text{font-size: 28rpx; color: #999;}





/* --- pop --- */
.pop{position: fixed; left: 0; top: 0; width: 100%; height: 100%; background: #000; z-index: 999; background-color:rgba(0,0,0,0.5);}
.pop_box{background: #fff; border-radius: 32rpx; padding: 32rpx; margin: 32rpx;}
.pop_top{margin-bottom: 32rpx;}
.pop_title{font-size: 32rpx; font-weight: bolder;}
.pop_close{width: 40rpx; height: 40rpx; margin-left: auto;}
.pop_content{font-size: 28rpx; line-height: 160%; color: #222; height: 60vh; overflow-y: auto;}




/* --- form --- */
.page_form__item{min-height: 96rpx; padding-left: 32rpx; background: #fff; position: relative;}
.page_form__title{ border-bottom: 1rpx solid #fff; width: 25%; height: 100%; line-height: 88rpx; font-size: 25rpx;}
.page_form__content{ border-bottom: 1rpx solid #efefef; width: 75%; height: 100%; line-height: 88rpx; font-size: 25rpx;}
.page_form__input{height: 96rpx; font-size: 28rpx;}
.page_form__textarea{min-height: 200rpx; padding-top: 32rpx;}




/* --- font --- */
@font-face{font-family:'Din'; src: url(data:application/font-ttf;charset=utf-8;base64,) format('truetype');}


/* --- 20202.3.1 --- */

/* unit */
.page-unit{font-size: 20rpx;}

/* price */
.group-price-tag{width: 80rpx; height: 32rpx; font-size: 20rpx; background-color:rgba(248,98,76,0.1); color: #FBA89C; border-radius: 8rpx; text-align: center; line-height: 32rpx; margin-right: 16rpx;}
.group-price-now{font-size: 32rpx; color: #F8624C; font-family: din; margin-right: 16rpx;}
.group-price-old{font-size: 32rpx; font-family: din; opacity: .3; text-decoration:line-through}


/* --- 字体 --- */
@font-face{
  font-family: 'DIN-Regular';
  src: url(http://mini.woqietu.com/chenfenqi/DIN-Regular.ttf)
}

@font-face{
  font-family: 'DIN-Bold';
  src: url(http://mini.woqietu.com/chenfenqi/DIN-Bold.ttf)
}

/* --- 默认设置 --- */
page{font-size: 32rpx; box-sizing: border-box;}
.flex{ display: flex; align-items: center; flex-wrap: wrap;}
.shadow{ box-shadow: 0rpx 6rpx 12rpx rgba(29, 29, 29, 0.07);}
.radius{border-radius: 16rpx }



/* --- button --- */
.page-button{margin: 0 32rpx; box-sizing: border-box;}
.page-button button{background: #5360FE; border-radius: 16rpx; color: #fff; font-size: 32rpx; height: 96rpx; line-height: 96rpx; display: flex; align-items: center; justify-content: center; padding: 0; margin: 0 32rpx;}



.page-button-1{margin: 0 32rpx; box-sizing: border-box; display: flex; justify-content: center;}


/* --- 栏目 --- */
.page-column{ padding: 32rpx;}

/* 标题 */
.page-column .title {font-size: 32rpx; font-weight: bolder; position: relative; padding-left: 25rpx; }
.page-column .title::before{content: ""; left: 0; top: 6rpx; height: 32rpx; width: 8rpx; background: #5360FE; position: absolute; border-radius: 100rpx;}

/* 更多 */
.page-column .more{margin-left: auto; background: url(http://mini.woqietu.com/chenfenqi/page-more-arrow.svg) no-repeat right center; padding-right: 32rpx; background-size: 12rpx 24rpx; font-size: 24rpx;}



/* --- 面板 --- */
.page-panel{ padding: 0 32rpx; margin-bottom: 32rpx;}
.page-panel .box{background: #fff; }
.page-panel .item{height: 112rpx; position: relative;}
.page-panel .item .icon{text-align: center; width:124rpx;}
.page-panel .item .icon image{ width:64rpx; height: 64rpx; }
.page-panel .item .text{border-bottom: 1px solid #efefef; font-size: 32rpx; position: relative; width: 560rpx; height: 112rpx; line-height: 112rpx;}
.page-panel .item .arrow{position: absolute ; right: 32rpx; top: 0; height: 100%; display: flex; align-items: center;}
.page-panel .item .arrow image{width: 40rpx; height: 40rpx;}



/* --- tab --- */
.page-tab {height: 96rpx; background: #fff; border-bottom: 1px solid #efefef; border-top: 1rpx solid #efefef; position: sticky; left: 0; top: 0; width: 100%; z-index: 300;}
.page-tab .item{ text-align: center; line-height: 96rpx; font-size: 28rpx; position: relative; display: flex; align-items: center; justify-content: center;}
.page-tab .item image{width: 32rpx; height: 32rpx; margin-left: 20rpx; margin-top: 16rpx;}
.page-tab .item.arrow{background: url(data:image/svg+xml;base64,) no-repeat 78% center;}

/* two */
.page-tab.two .item{ width: 33%; }

/* three */
.page-tab.three .item{ width: 33.333%; }

/* active */
.page-tab .item.active{color: #5360FE;}
.page-tab .item.active::after{content: ""; width: 20%; left: 40%; background: #5360FE; height: 6rpx; bottom: 0; position: absolute;}



/* --- none --- */
.page-none{padding: 80rpx 0; text-align: center;}
.page-none image{width: 480rpx; height: 480rpx;}



/* --- column --- */
.login-column{text-align: center; margin-bottom: 30rpx;}
.login-column .title{font-size: 64rpx; font-weight: bold;}
.login-column .dep{color: #979797; opacity: .4; font-family: 'DIN-Bold';}



/* --- form --- */
.page-form{border-radius: 32rpx 32rpx 0 0; height: 58.5vh; margin-top: -40rpx; padding: 60rpx; box-sizing: border-box; position: relative; z-index: 99999; background: #fff;}
.page-form .item{ border-bottom: 1rpx solid #efefef; height: 96rpx; line-height: 96rpx; position: relative; transition: all .3s;}
.page-form .item input{height: 100%; font-size: 30rpx;}
.page-form .item .verification{position: absolute; right: 0; top: 0; z-index: 300; font-size: 30rpx;}
.page-form .item .placeholder{color: #212121; opacity: .4;}
.page-form .item:hover{border-bottom: 1rpx solid #5460FE;}
.page-form .item .areaTips{font-size: 28rpx;}
.page-form .item .areaShow{font-size: 28rpx;}

/* button */
.page-form .button{margin-top: 56rpx;}
.page-form .button button{background: #5460FE; color: #fff; font-size: 32rpx; height: 96rpx; display: flex; align-items: center; justify-content: center;}

/* reg */
.page-form .reg{text-align: center; font-size: 30rpx; margin-top: 32rpx;}

/* tab */
.page-form .tab{text-align: center; font-size: 30rpx; margin-top: 32rpx; color: #999;}
.page-form .tab image{width: 24rpx; height: 24rpx;}

/* --- none --- */
.page-none{padding: 80rpx 0; text-align: center;}
.page-none image{width: 480rpx; height: 480rpx; margin: 0 auto;}
.page-none .title{text-align: center; font-size: 24rpx; padding-bottom: 80rpx;}